<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.knowledge.common.business.school.mapper.TbSchoolCalendarRecommendDao">

	<!-- 开启二级缓存
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> -->

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.knowledge.common.business.school.entity.TbSchoolCalendarRecommend">
		<id column="id" property="id" />
		<result column="title" property="title" />
		<result column="school" property="school" />
		<result column="activity_date" property="activityDate" />
		<result column="create_name" property="createName" />
		<result column="create_time" property="createTime" />
		<result column="status" property="status" />
		<result column="content" property="content" />
		<result column="outer_chain_url" property="outerChainUrl" />
		<result column="type" property="type" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, school, activity_date AS activityDate, create_name AS createName, create_time AS createTime, status, content, outer_chain_url AS outerChainUrl
    </sql>

</mapper>
