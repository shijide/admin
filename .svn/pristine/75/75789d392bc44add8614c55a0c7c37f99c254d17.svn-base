<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.knowledge.common.business.principalEnroll.mapper.TbPrincipalEnrollGroupDao">

	<!-- 开启二级缓存
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> -->

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.knowledge.common.business.principalEnroll.entity.TbPrincipalEnrollGroup">
		<id column="id" property="id" />
		<result column="enroll_id" property="enrollId" />
		<result column="name" property="name" />
		<result column="occupation" property="occupation" />
		<result column="region" property="region" />
		<result column="provinces" property="provinces" />
		<result column="school" property="school" />
		<result column="address" property="address" />
		<result column="phone" property="phone" />
		<result column="mail" property="mail" />
		<result column="certificates" property="certificates" />
		<result column="passcheck" property="passcheck" />
		<result column="picture_url" property="pictureUrl" />
		<result column="introduce" property="introduce" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, enroll_id AS enrollId, name, occupation, region, provinces, school, address, phone, mail, certificates, passcheck, picture_url AS pictureUrl, introduce
    </sql>

</mapper>
